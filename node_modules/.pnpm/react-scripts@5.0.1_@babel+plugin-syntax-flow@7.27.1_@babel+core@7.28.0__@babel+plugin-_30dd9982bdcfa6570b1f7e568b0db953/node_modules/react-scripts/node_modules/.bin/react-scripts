#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/flavien/Desktop/coach-platform/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+core@7.28.0__@babel+plugin-_30dd9982bdcfa6570b1f7e568b0db953/node_modules/react-scripts/bin/node_modules:/Users/flavien/Desktop/coach-platform/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+core@7.28.0__@babel+plugin-_30dd9982bdcfa6570b1f7e568b0db953/node_modules/react-scripts/node_modules:/Users/flavien/Desktop/coach-platform/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+core@7.28.0__@babel+plugin-_30dd9982bdcfa6570b1f7e568b0db953/node_modules:/Users/flavien/Desktop/coach-platform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/flavien/Desktop/coach-platform/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+core@7.28.0__@babel+plugin-_30dd9982bdcfa6570b1f7e568b0db953/node_modules/react-scripts/bin/node_modules:/Users/flavien/Desktop/coach-platform/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+core@7.28.0__@babel+plugin-_30dd9982bdcfa6570b1f7e568b0db953/node_modules/react-scripts/node_modules:/Users/flavien/Desktop/coach-platform/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+core@7.28.0__@babel+plugin-_30dd9982bdcfa6570b1f7e568b0db953/node_modules:/Users/flavien/Desktop/coach-platform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../../bin/react-scripts.js" "$@"
fi
