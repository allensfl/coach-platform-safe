{
  "buildCommand": "cd apps/dashboard && npm run build",
  "outputDirectory": "apps/dashboard/dist", 
  "installCommand": "npm install --legacy-peer-deps",
  "rewrites": [
    {
      "source": "/coachee/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)", 
      "destination": "/index.html"
    }
  ]
}